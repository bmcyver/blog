{
  "$schema": "./node_modules/wrangler/config-schema.json",
  "name": "blog",
  "main": "dist/_worker.js/index.js",
  "compatibility_date": "2025-12-04",
  "workers_dev": false,
  "assets": {
    "binding": "ASSETS",
    "directory": "./dist"
